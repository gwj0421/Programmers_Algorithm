    "import fractions\n",
    "import math\n",
    "\n",
    "def solution(w,h): \n",
    "    a=fractions.Fraction(w, h).denominator\n",
    "    b=fractions.Fraction(w, h).numerator\n",
    "    return w*h-(a+b-1)*math.gcd(w,h)\n",
