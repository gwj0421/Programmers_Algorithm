    "def primeSum(n):\n",
    "    prime_sum=0\n",
    "    for i in range(1,int(n**0.5+1)):\n",
    "        if n%i==0:\n",
    "            prime_sum+=i\n",
    "            if (i != (n // i)): \n",
    "                prime_sum+=n//i\n",
    "    return prime_sum\n",
    "    \n",
    "def solution(n):\n",
    "    return primeSum(n)"
