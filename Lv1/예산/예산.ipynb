    "def budget_allocation(blist,b):\n",
    "    blist.sort()\n",
    "    cnt=0\n",
    "    for i in blist:\n",
    "        b=b-i\n",
    "        if b>=0:\n",
    "            cnt+=1\n",
    "        else :\n",
    "            break\n",
    "    return cnt\n",
    "\n",
    "def solution(d, budget):\n",
    "    return budget_allocation(d,budget)"
   ]
