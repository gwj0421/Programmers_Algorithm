    "import itertools\n",
    "\n",
    "def indexCombination (num):\n",
    "    num=list(itertools.combinations(num, 2))\n",
    "    for i,j in enumerate(num):\n",
    "        num[i]=sum(j)\n",
    "        \n",
    "    return sorted(set(num))\n",
    "\n",
    "def solution(numbers):\n",
    "    return indexCombination(numbers)"
   ]
