    "import datetime\n",
    "\n",
    "def weekPredict(year,mon,day):\n",
    "    date=datetime.datetime(year,mon,day)\n",
    "    return date.weekday()\n",
    "\n",
    "def solution(a, b):\n",
    "    week=['MON','TUE','WED','THU','FRI','SAT','SUN']\n",
    "    return week[weekPredict(2016,a,b)]"
   ]
