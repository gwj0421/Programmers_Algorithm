    "def solution(nums):\n",
    "    answer = 0\n",
    "    temp=set(nums)\n",
    "    if len(temp)<len(nums)//2:\n",
    "        answer=len(temp)\n",
    "    else:\n",
    "        answer=len(nums)//2\n",
    "    return answer\n",
