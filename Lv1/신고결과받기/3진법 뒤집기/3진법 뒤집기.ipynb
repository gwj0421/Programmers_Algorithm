    "def convert(n, q):\n",
    "    rev_base = ''\n",
    "\n",
    "    while n > 0:\n",
    "        n, mod = divmod(n, q)\n",
    "        rev_base += str(mod)\n",
    "\n",
    "    return rev_base\n",
    "\n",
    "def solution(n):\n",
    "    return int(convert(n,3),3)"
